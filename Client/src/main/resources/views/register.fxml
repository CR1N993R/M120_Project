<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="148.0" prefWidth="207.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.tbz.client.frontend.controller.RegisterController">
    <TextField fx:id="usernameTb" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="177.0" promptText="Username" />
    <PasswordField fx:id="pwdTb" layoutX="14.0" layoutY="48.0" prefHeight="25.0" prefWidth="177.0" promptText="Password" />
    <Label layoutX="15.0" layoutY="92.0" onMouseClicked="#haveAccountClicked" prefHeight="17.0" prefWidth="177.0" text="Sie haben bereits einen Account?" underline="true" />
    <Button layoutX="14.0" layoutY="111.0" mnemonicParsing="false" onAction="#registerClicked" text="Register" />
   <Label fx:id="errorMSG" layoutX="14.0" layoutY="75.0" prefHeight="17.0" prefWidth="177.0" />
</AnchorPane>
